{
  "name": "fm-odata-client",
  "version": "0.1.0",
  "description": "FileMaker OData client developed by Soliant Consulting",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com:soliantconsulting/fm-odata-client.git"
  },
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "test": "mocha --loader=ts-node/esm --extension=ts test/**/*.ts",
    "coverage": "nyc npm test",
    "test-ci": "nyc --reporter=lcov npm test",
    "build": "rimraf ./build && tsc",
    "lint": "eslint .",
    "postinstall": "is-ci || husky install",
    "prepublishOnly": "npm test && npm run lint && pinst --disable",
    "postpublish": "pinst --enable",
    "prepare": "npm run build",
    "preversion": "npm run lint",
    "version": "npm run lint:fix && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint --cache --fix"
  },
  "author": "Soliant Consulting",
  "keywords": [
    "FileMaker",
    "REST",
    "API",
    "OData",
    "Typescript"
  ],
  "license": "MIT",
  "engines": {
    "node": ">=0.10"
  },
  "dependencies": {
    "file-type": "^16.5.3",
    "node-fetch": "^2.6.7",
    "tslib": "^2.1.0"
  },
  "peerDependencies": {
    "amazon-cognito-identity-js": "^4.5.12"
  },
  "peerDependenciesMeta": {
    "amazon-cognito-identity-js": {
      "optional": true
    }
  },
  "devDependencies": {
    "@tsconfig/node14": "^1.0.3",
    "@types/chai": "^4.2.15",
    "@types/chai-as-promised": "^7.1.3",
    "@types/mocha": "^9.1.1",
    "@types/nock": "^11.1.0",
    "@types/node": "^14.14.25",
    "@types/node-fetch": "^2.5.8",
    "@types/sinon": "^10.0.11",
    "amazon-cognito-identity-js": "^5.2.9",
    "chai": "^4.3.0",
    "chai-as-promised": "^7.1.1",
    "eslint": "^8.17.0",
    "eslint-config-dasprid": "^0.1.12",
    "husky": "^8.0.1",
    "is-ci": "^3.0.0",
    "lint-staged": "^13.0.1",
    "mocha": "^10.0.0",
    "nock": "^13.0.7",
    "nyc": "^15.1.0",
    "pinst": "^2.1.6",
    "rimraf": "^3.0.2",
    "sinon": "^14.0.0",
    "ts-node": "^10.8.1",
    "typescript": "^4.1.3"
  }
}
